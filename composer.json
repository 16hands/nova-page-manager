{
  "name": "optimistdigital/nova-page-manager",
  "description": "Page(s) and region(s) manager for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "page",
    "manager",
    "optimistdigital"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^2.9 || ^3.0 || ^4.0",
    "outl1ne/nova-translatable": "^2.0.4",
    "outl1ne/nova-translations-loader": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaPageManager\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaPageManager\\ToolServiceProvider"
      ],
      "aliases": {
        "NovaPageManagerCache": "OptimistDigital\\NovaPageManager\\Facade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
