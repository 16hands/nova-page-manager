{
  "name": "outl1ne/nova-page-manager",
  "description": "Page(s) and region(s) manager for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "page",
    "manager",
    "optimistdigital"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Outl1ne",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "outl1ne/nova-translatable": "^2.0.3",
    "outl1ne/nova-translations-loader": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\PageManager\\": "src/"
    },
    "files": [
      "src/Helpers/GlobalHelpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\PageManager\\NPMServiceProvider"
      ],
      "aliases": {
        "NPMCache": "Outl1ne\\PageManager\\Facades\\NPMCache"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
